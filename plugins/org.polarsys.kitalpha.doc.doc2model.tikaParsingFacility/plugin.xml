<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="tikaParser" name="Tika Parser" schema="schema/tikaParser.exsd"/>
   <extension
         point="org.polarsys.kitalpha.doc.doc2model.core.IParsingFacility">
      <parsingFacility
            IFormatDetector="TikaParsingFacility"
            IParsingFacility="org.polarsys.kitalpha.doc.doc2model.tikaParsingFacility.TikaParsingFacility">
      </parsingFacility>
   </extension>
   <extension
         point="org.polarsys.reqcycle.utils.inject.binding">
      <binding
            impl="org.polarsys.kitalpha.doc.doc2model.tikaParsingFacility.TikaParsingFacility"
            interface="org.polarsys.kitalpha.doc.doc2model.core.IParsingFacility">
      </binding>
   </extension>
   <extension
         point="org.polarsys.kitalpha.doc.doc2model.tikaParsingFacility.tikaParser">
      <parser
            instance="org.polarsys.kitalpha.doc.doc2model.tikaParsingFacility.doc.DocTikaParser">
      </parser>
      <parser
            instance="org.polarsys.kitalpha.doc.doc2model.tikaParsingFacility.xls.XLSTikaParser">
      </parser>
      <parser
            instance="org.polarsys.kitalpha.doc.doc2model.tikaParsingFacility.pdf.PDFTikaParser">
      </parser>
   </extension>

</plugin>
