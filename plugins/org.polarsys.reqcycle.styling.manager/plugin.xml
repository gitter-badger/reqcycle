<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated styling -->
      <package
            uri="http://www.polarsys.org/ReqCycle/styling/1.0"
            class="org.polarsys.reqcycle.styling.model.Styling.StylingPackage"
            genModel="model/styling.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <!-- @generated styling -->
      <factory
            uri="http://www.polarsys.org/ReqCycle/styling/1.0"
            class="org.polarsys.reqcycle.styling.model.Styling.provider.StylingItemProviderAdapterFactory"
            supportedTypes=
              "org.eclipse.emf.edit.provider.IEditingDomainItemProvider
               org.eclipse.emf.edit.provider.IStructuredItemContentProvider
               org.eclipse.emf.edit.provider.ITreeItemContentProvider
               org.eclipse.emf.edit.provider.IItemLabelProvider
               org.eclipse.emf.edit.provider.IItemPropertySource"/>
   </extension>
   <extension
         point="org.polarsys.reqcycle.utils.inject.binding">
      <binding
            impl="org.polarsys.reqcycle.styling.manager.impl.StylingManager"
            interface="org.polarsys.reqcycle.styling.manager.IStylingManager">
      </binding>
   </extension>
</plugin>
