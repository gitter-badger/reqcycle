<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
    <extension
         point="org.eclipse.ui.preferencePages">
		<page
            class="org.polarsys.reqcycle.core.ui.preferences.ReqCyclePreferencePage"
            id="org.eclipse.reqycle.core.ui.pref.reqcycle"
            name="ReqCycle">
      	</page>
   </extension>
   
   <extension
         point="org.eclipse.ui.menus">
    <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
      	<menu
               id="org.polarsys.reqcycle.core.ui.menu"
               label="ReqCycle"
               mnemonic="M">
          <command
                commandId="org.polarsys.reqcycle.core.ui.command.importConf"
                icon="icons/import_wiz-1.gif"
                label="Import Configuration"
                style="push">
          </command>
          <command
                commandId="org.polarsys.reqcycle.core.ui.command.exportConf"
                icon="icons/export_wiz-1.gif"
                label="Export Configuration"
                style="push">
          </command>
          <command
                commandId="org.polarsys.reqcycle.core.ui.command.initConf"
                icon="icons/thread_obj.gif"
                label="Initialize Configuration"
                style="push">
          </command>
          <command
                commandId="org.polarsys.reqcycle.core.ui.command.reqcyclePrefs"
                icon="icons/thread_view-1.gif"
                label="ReqCycle Preferences"
                style="push">
          </command>
      	</menu>
    </menuContribution>
	<menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
        <toolbar
              id="org.polarsys.reqcycle.core.ui.toolbar"
              label="ReqCycle">
		</toolbar>
	</menuContribution>
    </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.polarsys.reqcycle.core.ui.perspective.ReqCyclePerspective"
            icon="icons/reqcycle32.ico"
            id="org.polarsys.reqcycle.core.ui.ReqCyclePerspective"
            name="ReqCycle">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.polarsys.reqcycle.core.ui.category"
            name="ReqCycle Category">
      </category>
      <command
            categoryId="org.polarsys.reqcycle.core.ui.category"
            id="org.polarsys.reqcycle.core.ui.command.importConf"
            name="Import Configuration">
      </command>
      <command
            categoryId="org.polarsys.reqcycle.core.ui.category"
            id="org.polarsys.reqcycle.core.ui.command.exportConf"
            name="Export Configuration">
      </command>
      <command
            categoryId="org.polarsys.reqcycle.core.ui.category"
            id="org.polarsys.reqcycle.core.ui.command.initConf"
            name="Initialize configuration">
      </command>
      <command
            id="org.polarsys.reqcycle.core.ui.command.reqcyclePrefs"
            name="ReqCycle preferences">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.polarsys.reqcycle.core.ui.ImportConfigurationHandler"
            commandId="org.polarsys.reqcycle.core.ui.command.importConf">
      </handler>
      <handler
            class="org.polarsys.reqcycle.core.ui.ExportConfigurationHandler"
            commandId="org.polarsys.reqcycle.core.ui.command.exportConf">
      </handler>
      <handler
            class="org.polarsys.reqcycle.core.ui.InitializeConfigurationHandler"
            commandId="org.polarsys.reqcycle.core.ui.command.initConf">
      </handler>
      <handler
            class="org.polarsys.reqcycle.core.ui.ReqcyclePreferencesHandler"
            commandId="org.polarsys.reqcycle.core.ui.command.reqcyclePrefs">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            id="org.polarsys.reqcycle.core.ui.import"
            name="ReqCycle">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.exportWizards">
      <category
            id="org.polarsys.reqcycle.core.ui.export"
            name="ReqCycle">
      </category>
   </extension>
    
</plugin>
