<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.polarsys.reqcycle"
            class="org.polarsys.reqcycle.impact.ui.views.ImpactView"
            icon="icons/open.png"
            id="org.polarsys.reqcycle.impact.ui.views.impact"
            name="%view.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.polarsys.reqcycle.core.ui.menu">
         <separator
               name="org.polarsys.reqcycle.impact.ui.menu.start"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:org.polarsys.reqcycle.core.ui.menu?after=org.polarsys.reqcycle.impact.ui.menu.start">
         <command
               commandId="org.polarsys.reqcycle.impact.ui.OpenImpactAnalysis"
               icon="icons/open.png"
               label="%command.label"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.polarsys.reqcycle.impact.ui.handlers.OpenImpactAnalysis"
            commandId="org.polarsys.reqcycle.impact.ui.OpenImpactAnalysis">
      </handler>
   </extension>

</plugin>
